<div class="resumePage" style="width: 100%; height: 100%; background-color: #D3D3D3;" data-aos="fade-in">
    <div class="row">
        <img src="/bailey.jpg" class="rounded float-left" /> 
    </div>
    <div class="row" style="margin-top: 2%;"> 
        <div class="col-md-6 col-xs-12">
            <div class="card" style="font-family: 'Solway', serif;">
              <div class="card-header" style="background-color: #000000; color: white;">
			  <!--
				   job_title : JobTitle,
					job_compasny : JobCompany,
					job_time : JobTime,
					job_duties : JobDuties,
					college_name : CollegeName,
					start_date : StartDate, 
					end_date : EndDate,
			  
			  -->
                <h4>Current Job</h4>
              </div>
              <div class="card-body">
                <h5 class="card-title">Company: {{ job_compasny }}</h5>
